  "production": {
    "dialect": "postgres",
    "database": "friday_golf_prod",
    "host": "your_host",
    "port": 5432,
    "logging": false,
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  }
}
